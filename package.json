{
  "name": "choo-cli",
  "version": "0.8.0",
  "description": "create choo apps from the command line",
  "bin": {
    "choo": "bin/cli.js",
    "choo-new": "bin/choo-new.js",
    "choo-generate": "bin/choo-generate.js"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "chalkline": "0.0.5",
    "cli-spinners": "^0.3.0",
    "cross-spawn": "^5.0.1",
    "inquirer": "^1.2.3",
    "left-pad": "^1.1.1",
    "lodash": "^4.14.1",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^2.3.0",
    "minimist": "^1.2.0",
    "ramda": "^0.22.1",
    "resolve": "^1.1.7",
    "walk-back": "^2.0.1",
    "yamljs": "^0.2.8"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "clean": "rimraf temp && rimraf starter",
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "pretest": "npm run clean",
    "posttest": "npm run clean",
    "test": "standard | snazzy && NODE_ENV=test tape --require ./tests/**/*.js | tap-summary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trainyard/choo-cli.git"
  },
  "keywords": [
    "choo.js",
    "generator",
    "scaffold",
    "plop",
    "yeoman-generator",
    "slush",
    "cli",
    "choo",
    "boilerplate",
    "template"
  ],
  "author": "Matt McFarland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trainyard/choo-cli/issues"
  },
  "homepage": "https://github.com/trainyard/choo-cli#readme",
  "devDependencies": {
    "clinton": "^0.8.0",
    "dependency-check": "^2.6.0",
    "file-exists": "^2.0.0",
    "rimraf": "^2.5.4",
    "snazzy": "^5.0.0",
    "standard": "^8.0.0",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.0",
    "tape-catch": "^1.0.6"
  },
  "standard": {
    "ignore": [
      "template"
    ]
  }
}
